Windows Registry Editor Version 5.00
[HKEY_CLASSES_ROOT\getcookie]
@="URL:getcookie Protocol"
"URL Protocol"="---PATH---\\getcookie.exe"
 
[HKEY_CLASSES_ROOT\getcookie\DefaultIcon]
@="---PATH---\\getcookie.exe,1"
 
[HKEY_CLASSES_ROOT\getcookie\shell]
@="open"
 
[HKEY_CLASSES_ROOT\getcookie\shell\open]
@="open"
 
[HKEY_CLASSES_ROOT\getcookie\shell\open\command]
@="\"---PATH---\\getcookie.exe\" \"%1\""