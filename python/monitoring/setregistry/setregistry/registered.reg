Windows Registry Editor Version 5.00
[HKEY_CLASSES_ROOT\registered]
@="URL:registered Protocol"
"URL Protocol"="---PATH---\\registered.exe"
 
[HKEY_CLASSES_ROOT\registered\DefaultIcon]
@="---PATH---\\registered.exe,1"
 
[HKEY_CLASSES_ROOT\registered\shell]
@="open"
 
[HKEY_CLASSES_ROOT\registered\shell\open]
@="open"
 
[HKEY_CLASSES_ROOT\registered\shell\open\command]
@="\"---PATH---\\registered.exe\" \"%1\""